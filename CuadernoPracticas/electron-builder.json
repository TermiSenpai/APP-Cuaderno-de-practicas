{
  "appId": "com.xkoi.cuadernopracticas",
  "productName": "Cuaderno de pr√°cticas",
  "directories": {
    "output": "dist-electron",
    "buildResources": "public"
  },
  "files": [
    "dist/**/*",
    "electron/**/*",
    "package.json"
  ],
  "extraMetadata": {
    "main": "electron/main.cjs"
  },
  "win": {
    "target": [
      {
        "target": "portable",
        "arch": ["x64"]
      }
    ],
    "icon": "public/icons/icon.ico"
  },
  "compression": "store",
  "portable": {
    "artifactName": "${productName}-${version}-portable.exe",
    "unpackDirName": "${productName}"
  },
  "mac": {
    "target": ["dmg"],
    "icon": "public/icons/icon.icns",
    "category": "public.app-category.productivity"
  },
  "linux": {
    "target": ["AppImage", "deb"],
    "icon": "public/icons",
    "category": "Office"
  }
}
